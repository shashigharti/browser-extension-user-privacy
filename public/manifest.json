{
  "manifest_version": 2,
  "name": "OpenWPM",
  "version": "1.0",
  "description": "OpenWPM Client Extension",
  "background": {
    "scripts": ["main.js"]
  },
  "content_scripts": [],
  "applications": {
    "gecko": {
      "id": "openwpm@mozilla.org",
      "strict_min_version": "60.0"
    }
  },
  "content_security_policy":"script-src 'self' 'unsafe-eval'; object-src 'self';",
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "cookies",
    "management",
    "storage",
    "alarms",
    "downloads",
    "tabs",
    "dns"
  ]
}
