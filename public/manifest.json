{
  "manifest_version": 2,
  "name": "Privacy Web Extension",
  "version": "1.0",
  "description": "OpenWPM Client Extension",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "background": {
    "scripts": [
      "static/js/background.js"      
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js", "static/js/main.js", "static/js/content.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "cookies",
    "management",
    "storage",
    "alarms",
    "downloads",
    "tabs",
    "dns"
  ]
}